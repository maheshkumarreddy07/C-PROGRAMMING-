#include <stdio.h>
  
int main()
{
    
    int n = 11;
    int m = n - 1;
    int factm = 1;
   
    for (int i = 1; i <= m; i++) {
        factm *= i;
    }
  
  
    int factn = factm + 1;
    if (factn % n == 0) {
        
        printf(" %d  is prime number",n);
    }
    else {
        printf("%d  is not prime number",n);
    }
    return 0;
}
